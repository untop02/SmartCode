{
  "name": "smartcode",
  "displayName": "smartCode",
  "description": "The best no cap",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  "onView:smartCode.codeView!seen"],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "smart-code": [
        {
          "type": "webview",
          "id": "smartCode.codeView",
          "name": "Smart Code"
        }
      ]
    },
    "commands": [
      {
        "command": "smartCode.openView",
        "title": "Open Smart Code View"
      }
    ],
    "keybindings": [
      {
        "command": "smartCode.openView",
        "key": "ctrl+d",
        "mac": "cmd+d"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "type": "webview",
          "id": "smart-code",
          "title": "Smart Code",
          "icon": "cooltext456997901512876.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/uuid4": "^2.0.3",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "marked": "^12.0.1",
    "openai": "^4.31.0",
    "uuid4": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/untop02/SmartCode"
  }
}
